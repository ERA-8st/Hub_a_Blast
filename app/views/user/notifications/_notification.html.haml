- visiter = notification.visiter 

.user-view.clearfix
  %tr
    %td
      = link_to user_user_path(notification.visiter) do
        = attachment_image_tag( visiter , :profile_image,  size: "40x40", fallback: "no-image.png")
    %td
      = notification_form(notification)
      - if notification.message.present?
        %br
        .moderate-font.text-center{style: "color: #C0C0C0;"}
          = link_to user_room_path(notification.room_id, anchor: 'last') do
            %i.fa.fa-comments
          = notification.message
    %td
      .moderate-font= " (#{time_ago_in_words(notification.created_at)} Ââç)"